{
  "name": "@solidjs-hooks/solidjs-hooks",
  "version": "0.1.1",
  "license": "MIT",
  "author": "0xJWLabs <0xJWLabs@gmail.com>",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xJWLabs/solidjs-hooks.git"
  },
  "keywords": [
    "solid",
    "solid-js",
    "hooks",
    "reactivity",
    "react"
  ],
  "private": false,
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/server.cjs",
  "module": "./dist/server.js",
  "browser": {
    "./dist/server.js": "./dist/index.js",
    "./dist/server.cjs": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "worker": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/server.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/server.cjs"
      }
    },
    "browser": {
      "development": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/dev.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/dev.cjs"
        }
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "deno": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/server.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/server.cjs"
      }
    },
    "node": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/server.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/server.cjs"
      }
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/dev.cjs"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "prepublishOnly": "bun run build",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "vitest --mode=ssr",
    "format": "prettier -w {dev,src,test}/**/*.{js,ts,json,css,tsx,jsx} --ignore-path .gitignore",
    "lint": "eslint --ignore-path .gitignore --max-warnings 0 src/**/*.{js,ts,tsx,jsx}",
    "typecheck": "tsc --noEmit",
    "update-deps": "pnpm up -Li"
  },
  "devDependencies": {
    "@solid-primitives/destructure": "^0.1.17",
    "@solid-primitives/keyed": "^1.2.2",
    "@types/bun": "latest",
    "@types/node": "^22.7.6",
    "concurrently": "^9.0.1",
    "jsdom": "^25.0.1",
    "solid-devtools": "^0.30.1",
    "solid-js": "^1.9.2",
    "tsup": "^8.3.0",
    "tsup-preset-solid": "^2.2.0",
    "unocss": "^0.63.4",
    "vite": "^5.4.9",
    "vite-plugin-solid": "^2.10.2"
  },
  "peerDependencies": {
    "solid-js": "^1.9.2"
  }
}
